{
  "name": "vscode-calculator",
  "displayName": "VSCode calculator",
  "description": "Scientific calculator for vscode",
  "icon": "resources/calculator-icon.png",
  "publisher": "Dheovani",
  "author": {
    "name": "Dheovani Xavier da Cruz",
    "email": "dheovani_xavier@outlook.com"
  },
  "repository": {
    "directory": "Calculator",
    "url": "https://github.com/Dheovani/Calculator/tree/master"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "calculator-container",
          "title": "Calculator",
          "icon": "$(symbol-operator)"
        }
      ]
    },
    "views": {
      "calculator-container": [
        {
          "type": "webview",
          "id": "calculator",
          "name": "Calculator",
          "icon": "$(symbol-operator)"
        }
      ]
    }
  },
  "files": [
    "resources/calculator.html"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
